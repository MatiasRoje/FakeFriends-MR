  <% @room.users.each do |user| %>
    <% if user.photo.present? %>
    <li>
      <%= cl_image_tag user.photo.key, :aspect_ratio=>"1.0", :height=>300, :width=>300, crop: "fill", gravity: "faces", class:"player-avatar", alt:"player-img" %>
    </li>
    <% else %>
    <li>
      <img src="https://images.unsplash.com/photo-1633984726552-3ed7296dc5c9?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80" class="card-img" alt="Spaceship-img">
    </li>
    <% end %>
  <% end %>
  <% (6 - @room.users.count).times do %>
    <li><img src="https://images.unsplash.com/photo-1633984726552-3ed7296dc5c9?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80" class="placeholder-avatar" alt=""></li>
  <% end %>
