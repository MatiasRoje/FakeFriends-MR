<div id='stars'></div>
<div id='stars2'></div>
<div id='stars3'></div>

<div class="ff-container" data-controller="toggle-emoji">
  <div data-controller="ranking"
        data-ranking-target="friends"
        data-ranking-rankingroom-id-value="<%= @room.id %>">
    <!-- HERE THE PARTIAL WILL BE INSERTED -->
    <%= render "shared/ranking_room", winner: @winner, fakefriend: @fakefriend, room_message: @room_message %>
  </div>

  <div class="flex-btn-landing">
    <%= link_to new_room_path, class: "ff-btn-red extend-button" do %>
    <p>Play again?</p>
    <% end %>
  </div>

  <div class="flex-back-link">
    <%= link_to "<< BACK", root_path %>
  </div>
</div>
